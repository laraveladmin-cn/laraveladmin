    "less": "^4.1.2",
    "less-loader": "^7.0.0",
    "lodash": "^4.17.19",
    "popper.js": "^1.12",
    "postcss": "^8.3.11",
    "resolve-url-loader": "^3.1.0",
    "sass": "~1.26.5",
    "sass-loader": "^7.1.0",
    "tailwindcss": "^2.2.19",
    "vue": "^2.5.17",
    "vue-template-compiler": "^2.6.10"
  },
